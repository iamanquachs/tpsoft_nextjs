.scroll-container {
  position: fixed;
  z-index: 2000;
  top: 65px;
  left: 0;
  width: 100%;
  height: 5px;
  .indicator {
    height: 100%;
    background: linear-gradient(to right, #ff9900, #9900ff);
  }
}
.title_main{
  font-size: 40px !important;
}
.title_child{
  max-width: 400px;
  text-align: justify;
  border-left: 1px solid #ddd;
  padding-left: 30px;
  font-size: 30px !important;
  line-height: 50px !important;
}
.section_header {
  padding: 0 40px;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
  background-color: #fff;
  .section_header_left {
    height: 100%;
    display: flex;
    align-items: center;
  }
  .section_header_right {
    ul {
      display: flex;
      justify-content: space-between;
      align-items: center;
      list-style-type: none;
      margin: 0;
      text-transform: uppercase;
      li {
        display: flex;
        align-items: center;
        a {
          vertical-align: middle;
          padding: 20px;
          cursor: pointer;
          font-size: 16px;
          color: #333;
          text-decoration: none;
          &:hover {
            color: red;
            font-weight: bold;
          }
        }

        .lienhe {
          color: #ffff !important;
          background-color: #1435c3;
          padding: 4px 12px;
          border-radius: 20px;
        }
      }
      .active a{
        font-weight: bold;
        color: red;
      }
    }
  }
}

.section_header_mb {
  display: none;
  .section_header_left {
