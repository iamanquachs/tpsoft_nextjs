.section_home_info {
  background-attachment: fixed;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  overflow: hidden;
  height: 500px;
  display: flex;
  align-items: center;
  color: #fff;
  padding: 0 10px;

  .section_info_left {
    display: flex;
    align-items: center;
    gap: 30px;
    div {
      display: flex;
      flex-direction: column;
      align-items: center;
      line-height: 50px;
    }
    P {
      font-size: 15px;
      color: #cfbff7;
    }
    .border_bottom_info {
      background-color: #fff;
      width: 50px;
      height: 5px;
    }
  }
  .section_info_right {
    line-height: 50px;
    position: relative;
    padding: 20px 10px 20px 0px 20px;
    border: 8px solid #fff;
    border-left: 0px;
    div {
      padding: 40px 40px;
    }
    p {
      font-size: 20px;
      text-align: justify;
    }
    h4 {
      border-left: solid 3px #ff0005;
      padding: 0 10px;
    }
    ::before,
    ::after {
      content: "";
      position: absolute;
      background-color: #fff;
      left: 0px;
      width: 10px;
      height: 110px;
    }
    ::after {
      bottom: 0px;
    }
    ::before {
      top: 0px;
    }
  }
}

