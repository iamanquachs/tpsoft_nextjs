  }

  .section_header_mb.sticky {
    position: fixed;
    z-index: 1000;
    width: 100%;
    background-color: #fff;
    padding-top: 10px;
    box-shadow: 0 2px 2px 0px rgb(0 0 0 / 10%);
    .section_header_right {
      ul {
        li {
          a {
            color: #ffff;
          }
        }
      }
    }
  }
  .section_header {
    display: none;
  }
  .section_header_mb {
    position: sticky;
    position: -webkit-sticky;
    top: 0;
    z-index: 999;
    background-color: #ffff;
    display: block;
    box-shadow: 0 2px 2px 0px rgb(0 0 0 / 10%);
    .header_mb_top {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 10px 10px 10px 10px;
      border-bottom: 1px solid #ddd;
    }
    .header_mb_lienhe {
      display: flex;
      justify-content: center;
      align-items: center;
      gap: 5px;
      p {
        color: #333;
        font-family: var(--font_semi);
        margin: 0px 10px 0px 5px;
      }
    }
  }
  .header_mb_bottom {
    ul {
      list-style-type: none;
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 10px 20px;
      margin: 0;
      li {
        color: #000;
        font-size: 14px;
        text-decoration: unset;
        &:hover {
          cursor: pointer;
        }
      }

      .active {
        font-family: var(--font_semi);
        color: #B30000;
      }
    }
  }
  .scroll-container {
    top: 120px;
  }
  .section_whatwedo_left{
    display: flex ;
    justify-content: center;
  }
  .section_whatwedo_right{
    margin-top: 30px;
  }
  .section_culture_left{
    margin-top: 30px; 
    display: flex;
    justify-content: center;
    img{
    width: 700px !important;
    }
  }
  .bg_footer{
    .col_footer{
      padding: 20px;
    }
  }

  .img_tinnhiemmang{
    min-width: 200px;
    max-width: 230px;
  }
  .lienhe_header{
    li{
      list-style: none;
    }
  }
