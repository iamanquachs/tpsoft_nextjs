:root {
  --font_semi: "OpenSans-SemiBold", sans-serif;
}
.scroll-container {
  position: fixed;
  z-index: 2000;
  top: 65px;
  left: 0;
  width: 100%;
  height: 5px;
  .indicator {
    height: 100%;
    background: linear-gradient(to right, #ff9900, #9900ff);
  }
}
.title_main {
  font-size: 40px !important;
}
.title_child {
  max-width: 420px;
  text-align: justify;
  border-left: 1px solid #ddd;
  padding-left: 30px;
  font-size: 30px !important;
  line-height: 50px !important;
}
.section_header {
  padding: 5px 40px;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
  background-color: #fff;
  box-shadow: rgba(50, 50, 93, 0.25) 0px 6px 12px -2px, rgba(0, 0, 0, 0.3) 0px 3px 7px -3px;
  .section_header_left {
    height: 100%;
    display: flex;
    align-items: center;
  }
  .section_header_right {
    ul {
      display: flex;
      justify-content: space-between;
      align-items: center;
      list-style-type: none;
      margin: 0;
      text-transform: uppercase;
      li {
        display: flex;
        align-items: center;
        a {
          vertical-align: middle;
          padding: 20px;
          cursor: pointer;
          font-size: 16px;
          color: #333;
          text-decoration: none;
          &:hover {
            color: red;
          }
        }

        .lienhe {
          color: #ffff !important;
          background-color: #1435c3;
          padding: 4px 12px;
          border-radius: 20px;
        }
      }
      .active a {
        font-family: var(--font_semi);
        color: red;
      }
    }
  }
}

.section_header_mb {
  display: none;
  .section_header_left {
    padding: 12px 30px;
  }
  .fb {
    display: none;
  }
}

.intro_wrap {
