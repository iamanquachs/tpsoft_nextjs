.section_home_info{
  background-attachment: fixed;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  overflow: hidden;
  height: 500px;
  display: flex;
  align-items: center;
  color: #fff;
  .section_info_left{
    display: flex;
    align-items: center;
    gap: 20px;
    div{
      text-align: center;
      line-height: 50px;
    }
  }
  .section_info_right{
    line-height: 30px;
    
    position: relative;
    padding: 50px 30px 50px 50px;
    border: 10px solid #fff;
    border-left: 0px;
    div{
      padding: 20px;
    }
    ::before,::after{
      content: "";
      position: absolute;
      background-color: #fff;
      left: 0px;
      width: 10px;
      height: 110px;
    }
    ::after{
      bottom: 0px;
    }
    ::before{
      top: 0px;
    }
  }
}

